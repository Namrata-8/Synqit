    <stroke
        android:width="2dp"
        android:color="?attr/bg_dotted_circle"
        android:dashWidth="20dp"
        android:dashGap="15dp" />
    <padding
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp" />
    <corners android:radius="2dp" />
</shape>